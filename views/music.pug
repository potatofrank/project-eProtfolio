doctype html
head
    meta(charset='UTF-8')
    title Music
    link(rel='stylesheet', href='/stylesheets/music.css')
    style.
        body {
            background-color: #e1e1e1;
        }
        div{text-align:center;
            margin-top: 50px;
        }

    <div class="music-intro">
        <section>
        <h2>Music Sheet</h2>

        </section>
    </div>

    a(href='/') return to homepage

    if(files)
        each file in files
            .card.card-body.mb-3
                if(file.isImage)
                    h1 #{file.filename}
                    embed(src='pdf/' + file.filename width='50%' height='700')
    else
        p No files to show

